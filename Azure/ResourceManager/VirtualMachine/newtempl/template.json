{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_vm_adminPassword": {
            "defaultValue": "Access4theAdmin",
            "type": "String"
        },
        "virtualMachines_vm_name": {
            "defaultValue": "vm73",
            "type": "String"
        },
        "networkInterfaces_vm_name": {
            "defaultValue": "vm73",
            "type": "String"
        },
        "networkSecurityGroups_vm_name": {
            "defaultValue": "vm73",
            "type": "String"
        },
        "publicIPAddresses_vm_name": {
            "defaultValue": "vm73",
            "type": "String"
        },
        "storageAccounts_rg_name": {
            "defaultValue": "saverboon1",
            "type": "String"
        },
        "networkInterfaces_vm_id": {
            "defaultValue": "/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/RG_2/providers/Microsoft.Network/virtualNetworks/VNet1/subnets/Subnet-1",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/RG_1/providers/Microsoft.Compute/virtualMachines/vm1'.",
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[parameters('virtualMachines_vm_name')]",
            "apiVersion": "2015-06-15",
            "location": "westeurope",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Basic_A2"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftVisualStudio",
                        "offer": "Windows",
                        "sku": "10-Enterprise-N",
                        "version": "latest"
                    },
                    "osDisk": {
                        "name": "[parameters('virtualMachines_vm_name')]",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https://saverboon1.blob.core.windows.net/vhds/', parameters('virtualMachines_vm_name'),'2016423235548.vhd')]"
                        },
                        "caching": "ReadWrite"
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_vm_name')]",
                    "adminUsername": "Master_Admin",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "adminPassword": "[parameters('virtualMachines_vm_adminPassword')]"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_vm_name'))]"
                        }
                    ]
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_vm_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/RG_2/providers/Microsoft.Network/networkInterfaces/vm73'.",
            "type": "Microsoft.Network/networkInterfaces",
            "name": "[parameters('networkInterfaces_vm_name')]",
            "apiVersion": "2015-06-15",
            "location": "westeurope",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.0.0.8",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_vm_name'))]"
                            },
                            "subnet": {
                                "id": "[parameters('networkInterfaces_vm_id')]"
                            }
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableIPForwarding": false,
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_vm_name'))]"
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_vm_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_vm_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/RG_2/providers/Microsoft.Network/networkSecurityGroups/vm'.",
            "type": "Microsoft.Network/networkSecurityGroups",
            "name": "[parameters('networkSecurityGroups_vm_name')]",
            "apiVersion": "2015-06-15",
            "location": "westeurope",
            "properties": {
                "securityRules": [
                    {
                        "name": "default-allow-rdp",
                        "properties": {
                            "protocol": "Tcp",
                            "sourcePortRange": "*",
                            "destinationPortRange": "3389",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1000,
                            "direction": "Inbound"
                        }
                    }
                ]
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/RG_1/providers/Microsoft.Network/publicIPAddresses/vm1'.",
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[parameters('publicIPAddresses_vm_name')]",
            "apiVersion": "2015-06-15",
            "location": "westeurope",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "idleTimeoutInMinutes": 4
            },
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/fac764d2-d579-41e0-ba9d-d787cf339faf/resourceGroups/rg_1/providers/Microsoft.Storage/storageAccounts/rg11099'.",
            "type": "Microsoft.Storage/storageAccounts",
            "name": "[parameters('storageAccounts_rg_name')]",
            "apiVersion": "2015-06-15",
            "location": "westeurope",
            "tags": {},
            "properties": {
                "accountType": "Standard_LRS"
            },
            "dependsOn": []
        }
    ]
}